{
  "name": "endorsed weapons fork",
  "tree": {
    "$className": "DataModel",

    "Workspace": {
      "$className": "Workspace",
      "$path": "src/workspace"
    },

    "ReplicatedStorage": {
      "RequestBaseCreation": { "$className": "RemoteEvent" },
      "DisplayBasePrompt": { "$className": "RemoteEvent" },
      "UpdateBaseEntryGUI": { "$className": "RemoteEvent" },
      "OpenBaseShop": { "$className": "RemoteEvent" },
      "ChangeStealSpeed": { "$className": "RemoteEvent" },
      "ChangeMineSpeed": { "$className": "RemoteEvent" },
      "ChangeEntryTime": { "$className": "RemoteEvent" },
      "ChangeStorageSize": { "$className": "RemoteEvent" },
      "$className": "ReplicatedStorage",
      "$path": "src/shared"
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "WeaponsSystemServer": {
        "$path": "src/server"
      }
    },

    "ServerStorage": {
      "$className": "ServerStorage",
      "$path": "src/storage"
    },

    "StarterPack": {
      "$className": "StarterPack",
      "$path": "src/starterpack"
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "WeaponsSystemCamera": {
          "$path": "src/client"
        }
      }
    }
  }
}